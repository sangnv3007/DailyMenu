<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dailymenu">
    //Xin cấp quyền thực hiện cuộc gọi
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/chien"
        android:label="@string/app_name"
        android:roundIcon="@drawable/chien"
        android:supportsRtl="true"
        android:theme="@style/Theme.DailyMenu">
        <activity android:name=".activity_detailfood"></activity>
        <activity android:name=".activity_listfood" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>